  <!-- Page -->
  <div class="page animsition">
    <div class="page-content padding-30 container-fluid">
      <div class="row" data-plugin="matchHeight" data-by-row="true">
        
        <!-- Create Raffle -->
        <div class="col-xlg-5 col-md-6">
            <div class="panel" id = "create_raffle_panel">

                <div class="panel-heading">
                    <h3 class="panel-title">Create Raffle</h3>
                </div>
                <div class="panel-body">
                    <form id="frm_create_raffle" autocomplete="off">
                        <!--<div class="row form-group form-material">
                            <label class="control-label">Title<span class="required">*</span></label>
                            <input type="text" class="form-control" name="title" placeholder="Title of Raffle" required />
                        </div>-->
                        <div class="row form-group form-material">
                            <label class="control-label">Description<span class="required">*</span></label>
                            <textarea class="form-control" id="description" name="description" rows="3" placeholder="characters between 10 and 500" required></textarea>
                        </div>
                        <div class="row margin-top-20 form-group form-material">
                            <label class="control-label">Total Winners<span class="required">*</span></label>
                            <input type="text" class="form-control" id="winners_num" name="winners_num" data-fv-between="true" data-fv-between-min="1" data-fv-between-max="10" data-fv-between-message="please enter between[1,10]" placeholder="range = [1, 10]" data-fv-field="vRange" required>
                        </div>
                        <div class="row margin-top-20 form-group form-material">
                            <label class="control-label">Needed Raffles<span class="required">*</span></label>
                            <input type="text" class="form-control" id="raffles_num" name="raffles_num" data-fv-lessthan="true" data-fv-lessthan-value="50" data-fv-greaterthan-message="please enter less than 50" placeholder="max = 50" data-fv-field="vMax" required>
                        </div>
                        <div class="row margin-top-20 form-group form-material">
                            <label class="control-label">EndAt<span class="required">*</span></label>
                            <!--<input type="text" class="form-control" name="birthday" placeholder="YYYY/MM/DD" required />-->
                            <!--<div class="input-daterange-wrap">
                                <div class="input-daterange">
                                <div class="input-group">
                                    <span class="input-group-addon">
                                    <i class="icon md-calendar" aria-hidden="true"></i>
                                    </span>
                                    <input type="text" id="ending_date" name="ending_date" class="form-control datepair-date datepair-start" data-plugin="datepicker" required>
                                </div>
                                <div class="input-group">
                                    <span class="input-group-addon">
                                    <i class="icon md-time" aria-hidden="true"></i>
                                    </span>
                                    <input type="text" id="ending_time" name="ending_time" class="form-control datepair-time datepair-start ui-timepicker-input" data-plugin="timepicker" autocomplete="off" required>
                                </div>
                                </div>
                            </div>-->
                            <div class='input-group date' id='datetimepicker'>
                                <input type='text' class="form-control" name="ending_date" required/>
                                <span class="input-group-addon">
                                    <span class="glyphicon glyphicon-calendar"></span>
                                </span>
                            </div>
                        </div>
                        <div class="row margin-top-20">
                            <input type="file" id="input-file-now" data-plugin="dropify" data-default-file="" data-allowed-formats="landscape square" data-max-file-size="1M" data-max-height="1000" />
                            <p class="help">Only landscape or square images, 1M max and 1000px max-height.</p>
                        </div>
                        <div class="row margin-top-20 text-right">
                            <!-- Publish Button -->
                            <button type="submit" id="btn_submit" name="btn_submit"  class="btn btn-primary ladda-button" data-style="expand-left" data-plugin="ladda">
                              <span class="ladda-label">Publish</span>
                            <span class="ladda-spinner"></span></button>
                        </div>
                    </form>
                </div>
            </div>
        </div>

